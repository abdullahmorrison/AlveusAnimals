  "repository": {
    "type": "git",
    "url": "git+https://github.com/alveusgg/extension.git"
  },
  "bugs": {
    "url": "https://github.com/alveusgg/extension/issues"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "cross-env NODE_ENV=development webpack serve --config src/webpack.config.ts",
    "build": "webpack --config src/webpack.config.ts",
    "lint:eslint": "eslint src --ext .ts,.tsx",
    "lint:prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,scss}\"",
    "lint": "run-p lint:*",
    "format:eslint": "npm run lint:eslint -- --fix",
    "format:prettier": "npm run lint:prettier -- --write",
    "format": "run-p format:*"
  },
  "dependencies": {
    "@alveusgg/data": "github:alveusgg/data#0.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tmi.js": "^1.8.5"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@types/dotenv-webpack": "^7.0.6",
    "@types/node": "~18.17.19",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/tmi.js": "^1.8.5",
    "@types/twitch-ext": "^1.24.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.3.3",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.0.3",
    "react-refresh-typescript": "^2.0.9",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-image-resize-loader": "^5.0.0"
  },
  "main": "public/index.html",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
